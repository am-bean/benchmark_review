 doi = {10.18653/v1/2023.emnlp-main.726},
 editor = {Bouamor, Houda  and
Pino, Juan  and
Bali, Kalika},
 join_key = {331},
 month = {December},
 pages = {11843--11861},
 publisher = {Association for Computational Linguistics},
 title = {Can Language Models Understand Physical Concepts?},
