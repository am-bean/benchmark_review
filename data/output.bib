 editor = {Duh, Kevin  and
Gomez, Helena  and
Bethard, Steven},
 join_key = {521},
 month = {June},
 pages = {762--771},
 publisher = {Association for Computational Linguistics},
 title = {{M}u{L}an: A Study of Fact Mutability in Language Models},
