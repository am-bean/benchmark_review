 join_key = {290},
 month = {July},
 pages = {9126--9140},
 publisher = {Association for Computational Linguistics},
 title = {{W}ino{Q}ueer: A Community-in-the-Loop Benchmark for Anti-{LGBTQ}+ Bias in Large Language Models},
 url = {https://aclanthology.org/2023.acl-long.507/},
 year = {2023}
