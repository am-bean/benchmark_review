 join_key = {476},
 month = {November},
 pages = {15322--15336},
 publisher = {Association for Computational Linguistics},
 title = {{A}pp{B}ench: Planning of Multiple {API}s from Various {APP}s for Complex User Instruction},
 url = {https://aclanthology.org/2024.emnlp-main.856/},
 year = {2024}
